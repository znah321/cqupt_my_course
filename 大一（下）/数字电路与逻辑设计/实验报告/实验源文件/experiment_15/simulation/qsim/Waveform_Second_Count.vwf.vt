always @(posedge sampler_rx or negedge sampler_rx)
begin
`ifdef debug_tbench
	$display("Scanning pattern %d @time = %t",i,$realtime );
	i = i + 1;
	$display("| expected Carry_Sginal = %b | expected S = %b | ",Carry_Sginal_expected_prev,S_expected_prev);
