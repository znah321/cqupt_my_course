	if (
		( Carry_Sginal_expected_prev !== 1'bx ) && ( Carry_Sginal_prev !== Carry_Sginal_expected_prev )
		&& ((Carry_Sginal_expected_prev !== last_Carry_Sginal_exp) ||
			on_first_change[1])
	)
	begin
