begin
trigger = 0;
i = 0;
nummismatches = 0;
on_first_change = 2'b1;
end

// update real /o prevs

always @(trigger)
begin
	Carry_Sginal_prev = Carry_Sginal;
	S_prev = S;
end

// update expected /o prevs

always @(trigger)
begin
	Carry_Sginal_expected_prev = Carry_Sginal_expected;
	S_expected_prev = S_expected;
end



// expected Carry_Sginal
always
begin
	Carry_Sginal_expected = 1'b0;
